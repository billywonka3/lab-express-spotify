<div class="albums">
    <div id="graybar">
        <h3>Title</h3>
        <h3>Listen</h3>
    </div>
    {{#each track}}
        <div class="trackBar">
            <p>{{this.name}}</p>
            <audio controls>
                <source src={{this.preview_url}}>
            </audio>
        </div>
    {{/each}}
</div>